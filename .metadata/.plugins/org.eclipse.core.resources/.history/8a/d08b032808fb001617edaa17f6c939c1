package org.cb.practice;

public class Multi extends Thread {

	static int i;
	public void run() {
		
		for(i=1;i<=10;i++) {
		try{
			Thread.sleep(3000);
		}catch(InterruptedException e) {
			System.out.println(e);
		}
		System.out.println(i);
		System.out.println(Thread.currentThread());
	}
	}
	
	public static void main(String[] args) {
		
		Multi m1 = new Multi();
		Multi m2 = new Multi();
		
		m1.run();
		m2.run();
	//	m1.start();
		
		
		

	}

}
