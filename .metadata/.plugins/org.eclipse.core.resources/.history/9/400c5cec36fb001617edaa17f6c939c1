package org.cb.practice;

import java.io.IOException;

public class Multi {
	
	public static void main(String args[]) {
		
		Runtime r = Runtime.getRuntime();
		
		try {
			r.exec("C:\Windows\System32\shutdown -s -t o");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
	
}